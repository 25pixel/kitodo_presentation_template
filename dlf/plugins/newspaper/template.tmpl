            <tr>
                <td class="">###DAYMON###</td>
                <td class="">###DAYTUE###</td>
                <td class="">###DAYWED###</td>
                <td class="">###DAYTHU###</td>
                <td class="">###DAYFRI###</td>
                <td class="">###DAYSAT###</td>
                <td class="">###DAYSUN###</td>
            </tr>
            <!-- ###CALWEEK### end -->
